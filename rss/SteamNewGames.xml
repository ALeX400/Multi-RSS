   <video autoplay="" class="bb_img" height="266" loop="" muted="" playsinline="" poster="https://shared.fastly.steamstatic.com/store_item_assets/steam/apps/3456800/extras/61971c5f726d3a7ae6dc29663344b2f6.poster.avif?t=1755563692" width="800">
    <source src="https://shared.fastly.steamstatic.com/store_item_assets/steam/apps/3456800/extras/61971c5f726d3a7ae6dc29663344b2f6.webm?t=1755563692" type="video/webm; codecs=vp9"/>
