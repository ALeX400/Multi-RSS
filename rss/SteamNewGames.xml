   <video autoplay="" class="bb_img" height="502" loop="" muted="" playsinline="" poster="https://shared.fastly.steamstatic.com/store_item_assets/steam/apps/1346840/extras/fbe8e29f83ab2fc7f47b2c0b5be38afd.poster.avif?t=1757602304" width="1170">
    <source src="https://shared.fastly.steamstatic.com/store_item_assets/steam/apps/1346840/extras/fbe8e29f83ab2fc7f47b2c0b5be38afd.webm?t=1757602304" type="video/webm; codecs=vp9"/>
