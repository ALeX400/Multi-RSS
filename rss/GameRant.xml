     <source data-srcset="https://static0.gamerantimages.com/wordpress/wp-content/uploads/2024/09/fortnite_20240917161420-cropped.jpg?q=49&amp;fit=crop&amp;w=767&amp;dpr=2" media="(min-width: 481px)" srcset="https://static0.gamerantimages.com/wordpress/wp-content/uploads/2024/09/fortnite_20240917161420-cropped.jpg?q=49&amp;fit=crop&amp;w=767&amp;dpr=2"/>
     <source data-srcset="https://static0.gamerantimages.com/wordpress/wp-content/uploads/2024/09/fortnite_20240917161420-cropped.jpg?q=49&amp;fit=crop&amp;w=480&amp;dpr=2" media="(min-width: 0px)" srcset="https://static0.gamerantimages.com/wordpress/wp-content/uploads/2024/09/fortnite_20240917161420-cropped.jpg?q=49&amp;fit=crop&amp;w=480&amp;dpr=2"/>
     <img alt="iron man using the stark industries assault rifle" data-img-url="https://static0.gamerantimages.com/wordpress/wp-content/uploads/2024/09/fortnite_20240917161420-cropped.jpg" decoding="async" height="1920" loading="lazy" src="https://static0.gamerantimages.com/wordpress/wp-content/uploads/2024/09/fortnite_20240917161420-cropped.jpg" style="display:block;height:auto;max-width:100%;" width="3840"/>
    </picture>
